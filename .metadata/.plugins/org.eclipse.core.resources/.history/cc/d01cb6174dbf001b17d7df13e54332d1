public class CadernoDeEnderecos {
	//Atributos
	String nome;
	String telefone;
	String email;
	String endereco;
	boolean lotado;
	
	//Metodos
	String adicionaDados(){
		String dados = nome + "\n";
		dados += telefone + "\n"; 
		dados += email + "\n";
		dados += endereco + "\n";
		
		return dados;
	}	
	
	public void apresentaDados(){
		System.out.println(this.nome);
	}
	
	
}

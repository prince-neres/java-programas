import java.util.HashMap;
import java.util.Map;

public class Atividade3 {
	public static void main(String[] args) {
		Map<String,String> mapaNomes = new HashMap<String, String>();
		mapaNomes.put("17802387423", "João Delfino");
		mapaNomes.put("12343455677", "Maria do Carmo");
		mapaNomes.put("15234256576", "Claudinei Silva");

		System.out.println(mapaNomes);

	}
}
